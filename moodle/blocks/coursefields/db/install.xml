<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/coursefield/db" VERSION="20110221" COMMENT="XMLDB file for Moodle blocks/coursefierld"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
    <TABLES>
        <TABLE NAME="block_coursefield" COMMENT="Table for extra course field">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="partnerid"/>
                <FIELD NAME="partnerid" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="title"/>
                <FIELD NAME="title" TYPE="text" LENGTH="50" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="partnerid" NEXT="description"/>
                <FIELD NAME="description" TYPE="text" LENGTH="200" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="submission_b_id"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>