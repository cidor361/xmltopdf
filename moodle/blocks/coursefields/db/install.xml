<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/coursefields/db" VERSION="20110221" COMMENT="XMLDB file for Moodle blocks/coursefierlds"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
    <TABLES>
        <TABLE NAME="block_coursefields_main" COMMENT="Table for extra course field">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="true" NEXT="partnerid"/>
                <FIELD NAME="courseid" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="partnerid"/>
                <FIELD NAME="partnerid" TYPE="text" LENGTH="50" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="courseid" NEXT="title"/>
                <FIELD NAME="title" TYPE="text" LENGTH="20" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="partnerid" NEXT="started_at"/>
                <FIELD NAME="started_at" TYPE="text" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="title" NEXT="title"/>
                <FIELD NAME="finished_at" TYPE="text" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="started_at" NEXT="enrollment_finished_at"/>
                <FIELD NAME="enrollment_finished_at" TYPE="text" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="finished_at" NEXT="image"/>
                <FIELD NAME="image" TYPE="text" LENGTH="50" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="enrollment_finished_at" NEXT="description"/>
                <FIELD NAME="description" TYPE="text" LENGTH="100" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="image" NEXT="competences"/>
                <FIELD NAME="competences" TYPE="text" LENGTH="50" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="description" NEXT="requirements"/>
                <FIELD NAME="requirements" TYPE="text" LENGTH="100" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="content" TYPE="text" LENGTH="100" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="external_url" TYPE="text" LENGTH="100" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="direction" TYPE="text" LENGTH="50" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="institution" TYPE="text" LENGTH="50" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="duration" TYPE="text" LENGTH="50" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="lectures" TYPE="text" LENGTH="20" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="language" TYPE="text" LENGTH="2" NOTNULL="false" UNSIGNED="true"  SEQUENCE="false"/>
                <FIELD NAME="cert" TYPE="text" LENGTH="5" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="visitors" TYPE="text" LENGTH="20" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="teachers" TYPE="text" LENGTH="50" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="transfers" TYPE="text" LENGTH="20" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="results" TYPE="text" LENGTH="50" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="hours" TYPE="text" LENGTH="3" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="hours_per_week" TYPE="text" LENGTH="1" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="business_version" TYPE="text" LENGTH="100" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="promo_url" TYPE="text" LENGTH="100" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="promo_lang" TYPE="text" LENGTH="2" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="subtitles_lang" TYPE="text" LENGTH="2" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="estimation_tools" TYPE="text" LENGTH="20" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="proctoring_service" TYPE="text" LENGTH="50" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="sessionid" TYPE="text" LENGTH="50" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" FIELDS="courseid" TYPE="unique"/>
                <KEY NAME="id" FIELDS="id" TYPE="primary"/>
            </KEYS>
        </TABLE>
        <TABLE NAME="block_coursefields_teacher" COMMENT="Поля объекта teacher">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="true" NEXT="partnerid"/>
                <FIELD NAME="courseid" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="t_title" TYPE="text" LENGTH="255" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" />
                <FIELD NAME="t_image" TYPE="text" LENGTH="255" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="t_description" TYPE="text" LENGTH="255" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" FIELDS="courseid" TYPE="unique"/>
                <KEY NAME="id" FIELDS="id" TYPE="primary"/>
            </KEYS>
        </TABLE>
        <TABLE NAME="block_coursefields_coursetr" COMMENT="Поля объекта CourseTransfer">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="true" NEXT="partnerid"/>
                <FIELD NAME="courseid" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"/>
                <FIELD NAME="institution_id" TYPE="text" LENGTH="100" NOTNULL="true" UNSIGNED="false" SEQUENCE="false"/>
                <FIELD NAME="direction_id" TYPE="text" LENGTH="100" NOTNULL="true" UNSIGNED="false" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" FIELDS="courseid" TYPE="unique"/>
                <KEY NAME="id" FIELDS="id" TYPE="primary"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>